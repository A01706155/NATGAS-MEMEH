<%- include('partials/head.ejs') %>
<%- include('partials/navbar.ejs') %>

    <h1 class="center white-text"><%= titulo %></h1>

    <div class="container highlight proyectContent">
        <div style="margin-bottom: 0px;">
             
            <div class="white-text col md-12 sm-12 lg-9">
                
                <table class="responsive-table">
                    <tr>
                    <td>Nombre</td>
                    </tr>
                    <tr> 
                        <% for (let detalle of Fase) { %>

                            <td><%= detalle.nombreFase %> </td>

                        <% } %>          
                    </tr>      
                </table>   

                <br><br>
                
                <div class="center">
                    <% for (let detalle of Fase) { %>
                    <a href="/fases/registrar-fase/<%= detalle.idProyecto %>" class="waves-effect waves-light btn-large"><i class="material-icons left">edit</i>Agregar fase</a>
                    <a href="/fases/modificar-fase/<%= detalle.idFase %>" class="waves-effect waves-light yellow darken-4 btn-large"><i class="material-icons left">edit</i>Editar</a>
                    <br><br>
                    <a href="/proyectos/<%= detalle.idProyecto %>" class="waves-effect waves-light red darken-2 btn"><i class="material-icons left">undo</i>Regresar</a>        
                    <% } %>     
                </div>
                
            </div>
        </div>
    </div>    


<%- include('partials/footer.ejs') %>